<h1>Brands</h1>

<div class = "brands">
    {{#each brands}}
    <a href="/brands/{{_id}}">
        <div class = "item">
            <img src={{logoImg}}/>
            <h2>{{name}}</h2>
        </div>
    </a>
    {{/each}}
</div>

<a href="/brands/add">
    <button>
        Add New Brand
    </button>
</a>